{
    "tabWidth": 4,
    "useTabs": false,
    "endOfLine": "lf",
    "eslintIntegration": true,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": true,
    "printWidth": 140,
    "singleQuote": true,
    "tslintIntegration": true,
    "plugins": ["prettier-plugin-astro"],
    "overrides": [
        {
            "files": "*.astro",
            "options": {
                "parser": "astro"
            }
        }
    ]
}

